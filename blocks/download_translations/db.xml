<?xml version="1.0" encoding="UTF-8"?>
<schema
  xmlns="http://www.concrete5.org/doctrine-xml/0.5"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>

  <table name="btCTDownloadTranslations">
    <field name="bID" type="integer">
      <unsigned/>
      <key/>
    </field>
    <field name="allowedUsers" type="string" size="20">
    	<notnull/>
    </field>
	<field name="packageHandle" type="string" size="64">
    	<notnull/>
    </field>
    <field name="packageHandleFixed" type="boolean">
    	<notnull/>
    </field>
	<field name="packageVersion" type="string" size="64">
    	<notnull/>
    </field>
	<field name="packageVersionFixed" type="boolean">
    	<notnull/>
    </field>
	<field name="allowedFormats" type="text">
    	<notnull/>
    </field>
  </table>

</schema>
