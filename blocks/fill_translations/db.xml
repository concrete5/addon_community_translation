<?xml version="1.0" encoding="UTF-8"?>
<schema
    xmlns="http://www.concrete5.org/doctrine-xml/0.5"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.concrete5.org/doctrine-xml/0.5 http://concrete5.github.io/doctrine-xml/doctrine-xml-0.5.xsd"
>

    <table name="btCTFillTranslations">
        <field name="bID" type="integer">
            <unsigned/>
            <key/>
        </field>
        <field name="rateLimit_maxRequests" type="integer">
            <unsigned/>
        </field>
        <field name="rateLimit_timeWindow" type="integer">
            <unsigned/>
            <default value="3600"/>
            <notnull/>
        </field>
        <field name="maxFileSize" type="integer">
            <unsigned/>
        </field>
            <field name="maxLocalesCount" type="integer">
            <unsigned/>
        </field>
        <field name="maxStringsCount" type="integer">
            <unsigned/>
        </field>
    </table>

</schema>
